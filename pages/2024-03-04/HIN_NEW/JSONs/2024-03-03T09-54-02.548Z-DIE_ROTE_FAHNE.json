c\u0917\u093e\u0930\u0940 \u092e\u0947\u0902 \u0932\u0917\u093e\u0924\u093e\u0930 \u0935\u0943\u0926\u094d\u0927\u093f \u0939\u0941\u0908 - \u0906\u0927\u093e \u092e\u093f\u0932\u093f\u092f\u0928 \u0938\u0947 \u0905\u0927\u093f\u0915 \u092c\u0947\u0930\u094b\u091c\u0917\u093e\u0930 \u0925\u0947\u0964 \u0938\u0947\u0902\u0938\u0930\u0936\u093f\u092a \u0925\u0940, \u0905\u092d\u093f\u0935\u094d\u092f\u0915\u094d\u0924\u093f \u0915\u0940 \u0938\u094d\u0935\u0924\u0902\u0924\u094d\u0930\u0924\u093e \u0915\u094b \u092c\u0902\u0926 \u0915\u0930 \u0926\u093f\u092f\u093e \u0917\u092f\u093e \u0925\u093e, \u092a\u094d\u0930\u0947\u0938 \u0915\u0940 \u0938\u094d\u0935\u0924\u0902\u0924\u094d\u0930\u0924\u093e \u0915\u094b \u0939\u091f\u093e \u0926\u093f\u092f\u093e \u0917\u092f\u093e \u0925\u093e, \u0939\u0947\u0907\u092e\u0935\u0947\u0939\u0930 \u0928\u0947 \u0928\u090f \"\u0915\u093e\u0928\u0942\u0928\u094b\u0902\" \u0915\u094b \u0905\u0902\u091c\u093e\u092e \u0926\u093f\u092f\u093e, \u0938\u094d\u0925\u093f\u0924\u093f \u092a\u0947\u0936 \u0915\u0940 \u0917\u0908\u0964 \u0939\u093e\u0932\u093e\u0902\u0915\u093f, \u0936\u094d\u0930\u092e\u093f\u0915\u094b\u0902 \u0915\u094b \u0938\u093e\u092e\u093e\u091c\u093f\u0915 \u0932\u094b\u0915\u0924\u093e\u0902\u0924\u094d\u0930\u093f\u0915 \u0928\u0947\u0924\u0943\u0924\u094d\u0935 \u0926\u094d\u0935\u093e\u0930\u093e \u0938\u0902\u0918\u0930\u094d\u0937 \u0938\u0947 \u0935\u093e\u092a\u0938 \u0930\u0916\u093e \u0917\u092f\u093e \u0925\u093e\u0964 \u092b\u093f\u0930 \u092d\u0940, \u092b\u093e\u0938\u0940\u0935\u093e\u0926 \u0915\u0947 \u0916\u093f\u0932\u093e\u092b \u090f\u0915 \u0935\u0940\u0930 \u092a\u094d\u0930\u0924\u093f\u0930\u094b\u0927 \u0915\u093e \u0906\u092f\u094b\u091c\u0928 \u0915\u093f\u092f\u093e \u0917\u092f\u093e\u0964 \u0936\u094d\u0930\u092e\u093f\u0915\u094b\u0902 \u0915\u093e \u0938\u0902\u0918\u0930\u094d\u0937, \u0938\u0941\u0930\u0915\u094d\u0937\u093e\u0924\u094d\u092e\u0915 \u092c\u0902\u0921\u0932\u094b\u0902 \u0914\u0930 \u0915\u092e\u094d\u092f\u0941\u0928\u093f\u0938\u094d\u091f \u0915\u094b \u0930\u0915\u094d\u0924\u0939\u0940\u0928 \u0930\u0942\u092a \u0938\u0947 \u0909\u0926\u093e\u0938 \u0915\u0930 \u0926\u093f\u092f\u093e \u0917\u092f\u093e \u0925\u093e, \u0932\u0947\u0915\u093f\u0928 \u0911\u0938\u094d\u091f\u094d\u0930\u094b\u092b\u093e\u0938\u093f\u091c\u094d\u092e \u0915\u0947 \u0916\u093f\u0932\u093e\u092b \u092a\u094d\u0930\u0924\u093f\u0930\u094b\u0927 \u0915\u0940 \u0906\u0917\u0947 \u0915\u0940 \u0938\u0902\u0930\u091a\u0928\u093e \u0914\u0930 \u0930\u093e\u0937\u094d\u091f\u094d\u0930\u0940\u092f \u0938\u092e\u093e\u091c\u0935\u093e\u0926\u093f\u092f\u094b\u0902 \u0926\u094d\u0935\u093e\u0930\u093e \u0915\u092c\u094d\u091c\u0947 \u0915\u0947 \u092a\u094d\u0930\u0924\u093f\u0930\u094b\u0927 \u0915\u0947 \u0932\u093f\u090f \u0906\u0927\u093e\u0930 \u0915\u093e \u0917\u0920\u0928 \u0915\u093f\u092f\u093e\u0964 \u0907\u0938 \u0938\u0902\u092c\u0902\u0927 \u092e\u0947\u0902, \u092b\u0930\u0935\u0930\u0940 1934 \u092b\u093e\u0938\u0940\u0935\u093e\u0926 \u0915\u0947 \u0916\u093f\u0932\u093e\u092b \u0932\u0921\u093c\u093e\u0908 \u092e\u0947\u0902 \u090f\u0915 \u092e\u0939\u0924\u094d\u0935\u092a\u0942\u0930\u094d\u0923 \u0914\u0930 \u0936\u093e\u0928\u0926\u093e\u0930 \u091c\u0940\u0924 \u0925\u0940\u0964 \u0938\u0936\u0938\u094d\u0924\u094d\u0930 \u0938\u0902\u0918\u0930\u094d\u0937 \u090f\u0915 \"\u0917\u0932\u0924\u0940\" \u0928\u0939\u0940\u0902 \u0925\u093e, \u091c\u0948\u0938\u093e \u0915\u093f \u0905\u0915\u094d\u0938\u0930 \u0915\u0939\u0928\u0947 \u0915\u093e \u092a\u094d\u0930\u092f\u093e\u0938 \u0915\u093f\u092f\u093e \u091c\u093e\u0924\u093e \u0939\u0948, \u0932\u0947\u0915\u093f\u0928 \u0905\u0917\u0930 \u092f\u0939 \u0915\u094d\u0930\u093e\u0902\u0924\u093f\u0915\u093e\u0930\u0940 \u0924\u0930\u0940\u0915\u0947 \u0938\u0947 \u0928\u0947\u0924\u0943\u0924\u094d\u0935 \u0915\u093f\u092f\u093e \u0917\u092f\u093e \u0939\u094b\u0924\u093e \u0924\