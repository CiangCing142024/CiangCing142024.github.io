u043e\u043d\u0430\u0434 5 \u043c\u0456\u043b\u044c\u0439\u043e\u043d\u0456\u0432 \u0433\u0443\u043b\u044f\u043a\u0456\u0432. \u0417\u0430 \u0434\u0430\u043d\u0438\u043c\u0438 \u041a\u043e\u043c\u0456\u0442\u0435\u0442\u0443 \u0441\u043e\u043b\u0456\u0434\u0430\u0440\u043d\u043e\u0441\u0442\u0456 \u041c\u0456\u043d\u0430\u0441\u0430 \u0413\u0435\u0440\u0430\u0439\u0441\u0430 \u0434\u043e \u043f\u0430\u043b\u0435\u0441\u0442\u0438\u043d\u0441\u044c\u043a\u043e\u0433\u043e \u043d\u0430\u0440\u043e\u0434\u0443, \u0443 \u043f\u0443\u0431\u043b\u0456\u043a\u0430\u0446\u0456\u0457 \u0434\u043b\u044f \u0441\u0432\u043e\u0457\u0445 \u0441\u043e\u0446\u0456\u0430\u043b\u044c\u043d\u0438\u0445 \u043c\u0435\u0440\u0435\u0436:\n  <em>\n   \"\u041f\u0440\u0438\u0441\u0443\u0442\u043d\u0456\u0441\u0442\u044c \u043f\u0430\u043b\u0435\u0441\u0442\u0438\u043d\u0441\u044c\u043a\u043e\u0433\u043e \u043f\u0440\u0430\u043f\u043e\u0440\u0430 \u0442\u0430 \u0443\u0441\u043d\u0438\u0445 \u0432\u0442\u0440\u0443\u0447\u0430\u043d\u044c \u043f\u0456\u0434 \u0447\u0430\u0441 \u043f\u0430\u0440\u0430\u0434\u0456\u0432 \u0431\u0443\u043b\u0430 \u0434\u0443\u0436\u0435 \u0434\u043e\u0431\u0440\u0435 \u0441\u043f\u0440\u0438\u0439\u043d\u044f\u0442\u0430 \u0433\u0440\u043e\u043c\u0430\u0434\u0441\u044c\u043a\u0456\u0441\u0442\u044e!\"\n  </em>\n  \u0406 \u043d\u0430\u0432\u0456\u0442\u044c \u0446\u0435\n  <em>\n   \"\u0411\u0430\u0433\u0430\u0442\u043e \u0446\u0456\u043a\u0430\u0432\u0438\u0445 \u043b\u044e\u0434\u0435\u0439 \u0437\u0430\u043f\u0438\u0442\u0443\u044e\u0442\u044c, \u0449\u043e \u043d\u0430\u0441\u043f\u0440\u0430\u0432\u0434\u0456 \u0432\u0456\u0434\u0431\u0443\u0432\u0430\u0454\u0442\u044c\u0441\u044f \u0432 \u041f\u0430\u043b\u0435\u0441\u0442\u0438\u043d\u0456 ... \u0432\u043e\u043d\u0438 \u0437\u0430\u043f\u0438\u0442\u0443\u044e\u0442\u044c \u043d\u0430\u043a\u043b\u0435\u0439\u043a\u0438, \u0437\u0430\u043f\u0438\u0442\u0443\u044e\u0442\u044c, \u044f\u043a \u0432\u043e\u043d\u0438 \u043c\u043e\u0436\u0443\u0442\u044c \u0434\u043e\u043f\u043e\u043c\u043e\u0433\u0442\u0438\".\n  </em>\n </p>\n <p>\n  \u041a\u043e\u043c\u0456\u0442\u0435\u0442 \u043f\u0440\u043e\u0434\u043e\u0432\u0436\u0443\u0454 \u0441\u043f\u0440\u0438\u044f\u0442\u0438 \u0434\u0456\u044f\u043b\u044c\u043d\u043e\u0441\u0442\u0456, \u0449\u043e \u0432\u0438\u043c\u0430\u0433\u0430\u0454 \u0437\u0430\u043a\u0456\u043d\u0447\u0435\u043d\u043d\u044f \u0433\u0435\u043d\u043e\u0446\u0438\u0434\u0443, \u043d\u0435\u0433\u0430\u0439\u043d\u043e\u0433\u043e \u043f\u0440\u0438\u043f\u0438\u043d\u0435\u043d\u043d\u044f \u0432\u043e\u0433\u043d\u044e \u0442\u0430 \u043a\u0456\u043d\u0446\u044f \u0441\u0456\u0432\u043e\u043d\u0456\u0441\u0442\u0441\u044c\u043a\u043e\u0457 \u043e\u043a\u0443\u043f\u0430\u0446\u0456\u0457. \u0429\u043e\u0434\u043d\u044f, \u043d\u0430 7 \u0434\u0435\u043c\u0431\u0440\u043e -\u043f\u043b\u043e\u0449\u0456, \u0443 \u0433\u0456\u043f\u0435\u0440\u0446\u0435\u043d\u0442\u0440\u0456 \u0441\u0442\u043e\u043b\u0438\u0446\u0456 \u041a\u043e\u043c\u0456\u0442\u0435\u0442 \u041c\u0456\u043d\u0430\u0441 \u0413\u0435\u0440\u0430\u0439\u0441 \u0437\u0431\u0438\u0440\u0430\u0454 \u043f\u0456\u0434\u043f\u0438\u0441\u0438, \u0449\u043e \u0432\u0438\u043c\u0430\u0433\u0430\u0454 \u0440\u043e\u0437\u0431\u0438\u0442\u0442\u044f \u0431\u0440\u0430\u0437\u0438\u043b\u044c\u0441\u044c\u043a\u0438\u0445 \u0434\u0435\u0440\u0436\u0430\u0432\u043d\u0438\u0445 \u0432\u0456\u0434\u043d\u043e\u0441\u0438\u043d \u0437 \u00ab\u0406\u0437\u0440\u0430\u0457\u043b\u0435\u043c\u00bb, \u0440\u043e\u0437\u043f\u043e\u0434\u0456\u043b\u044f\u0454 \u043b\u0438\u0441\u0442\u0456\u0432\u043a\u0438 \u0442\u0430 \u0440\u043e\u0437\u043c\u043e\u0432\u0443 \u0437 \u043d\u0430\u0441\u0435\u043b\u0435\u043d\u043d\u044f\u043c.\n </p>\n <figure class=\"wp-block-gallery has-nested-images columns-default is-cropped wp-block-gallery-2 is-layout-flex wp-block-gallery-is-layout-flex\">\n  <figure class